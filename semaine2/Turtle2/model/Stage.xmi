<?xml version="1.0" encoding="UTF-8"?>
<turtle:Stage
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:turtle="http:///turtle.ecore"
    xsi:schemaLocation="http:///turtle.ecore turtle.ecore"
    backgroundColor="//@color">
  <color/>
  <choreographies
      name="choreo">
    <actions
        xsi:type="turtle:SetPen"
        state="DOWN"/>
    <actions
        xsi:type="turtle:Forward"
        length="100"/>
    <actions
        xsi:type="turtle:SetPen"/>
  </choreographies>
</turtle:Stage>
